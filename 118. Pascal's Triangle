class Solution {
public:
long long nCr(int numRows,int r){
    long long res=1;
    for(int i=0;i<r;i++){
        res = res*(numRows-i);
        res = res/(i+1);
    }
    return res;
}
    vector<vector<int>> generate(int numRows) {
        vector<vector<int>> triangle;
        for(int i=0;i<numRows;i++){
            vector<int> row;
            for(int j=0;j<=i;j++){
                row.push_back(nCr(i,j));
            }
            triangle.push_back(row);
        }
        return triangle;
    }
};
